    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.23",
    "if-env": "^1.0.4"
